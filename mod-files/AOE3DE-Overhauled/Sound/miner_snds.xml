<?xml version='1.0' encoding='utf-8'?>
<protounitsounddef>
  <protounit name="Miner">
    <soundtype name="Select">
        <soundset name="DESwedishSettlerMaleSelect"/>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="SettlerBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
		<soundset name="DESwedishSettlerMaleAcknowledge"/>
        </choice>
        <choice name="enemy">
        	<soundset name="DESwedishSettlerMaleAttack"/>
        </choice>
        <choice name="build">
                <soundset name="DESwedishSettlerMaleBuild"/>
        </choice>
        <choice name="Huntable">
                <soundset name="DESwedishSettlerMaleGatherMeat"/>
        </choice>
        <choice name="Tree">
                <soundset name="DESwedishSettlerMaleGatherWood"/>
        </choice>
        <choice name="AbstractMine">
                <soundset name="DESwedishSettlerMaleGatherCoin"/>
        </choice>
        <choice name="AbstractFarm">
                <soundset name="DESwedishSettlerMaleFarm"/>
        </choice>
        <choice name="AbstractResourceCrate">
                <soundset name="DESwedishSettlerMaleAcknowledge"/>
        </choice>
        <choice name="AbstractFruit">
        	<soundset name="DESwedishSettlerMaleGatherFruit"/>
        </choice>
      </targetlogic>
    </soundtype>
  </protounit>
</protounitsounddef>
