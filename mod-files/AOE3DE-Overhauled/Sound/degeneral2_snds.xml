<?xml version='1.0' encoding='utf-8'?>
<protounitsounddef>
  <protounit name="deGeneral2">
    <soundtype name="Select">
      <soundset name="DEAmericanGeneralSelect"/>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="DEAmericanGeneralAcknowledge"/>
        </choice>
        <choice name="enemy">
          <soundset name="DEAmericanGeneralAttack"/>
        </choice>
      </targetlogic>
    </soundtype>
    <soundtype name="KnockOut">
      <soundset name="DEAmericanGeneralDisabled"/>
    </soundtype>
    <soundtype name="KnockOutRevived">
      <soundset name="DEAmericanGeneralRevived"/>
    </soundtype>
    <soundtype name="Ransomed">
      <soundset name="DEAmericanGeneralRansomed"/>
    </soundtype>
  </protounit>
</protounitsounddef>
