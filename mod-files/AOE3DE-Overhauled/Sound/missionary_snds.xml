<?xml version='1.0' encoding='utf-8'?>
<protounitsounddef>
  <protounit name="Missionary">
    <soundtype name="Select">
      <civlogic>
        <choice name="Spanish">
          <soundset name="SpanishPriestSelect"/>
        </choice>
        <choice name="DEMexicans">
          <soundset name="DEMexicanPadreSelect"/>
        </choice>
      </civlogic>
    </soundtype>    
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="SettlerBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice name="Spanish">
          <targetlogic>
            <choice name="default">
              <soundset name="SpanishPriestAcknowledge"/>
            </choice>
            <choice name="enemy"/>
          </targetlogic>
        </choice>
        <choice name="DEMexicans">
          <targetlogic>
            <choice name="default">
              <soundset name="DEMexicanPadreAcknowledge"/>
            </choice>
            <choice name="enemy">
              <soundset name="DEMexicanPadreAttack"/>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
    </soundtype>
  </protounit>
</protounitsounddef>
